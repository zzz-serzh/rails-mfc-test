<h1>список МФЦ</h1>
 
<table>
  <tr>
    <th>Название</th>
    <th>ЕСИА</th>
    <th>Директор</th>
    <th>ОГРН</th>
  </tr>
 
  <% @mfcoffices.each do |mfcoffice| %>
    <tr>
      <td><%= mfcoffice.name %></td>
      <td><%= mfcoffice.esia %></td>
      <td><%= mfcoffice.director %></td>
      <td><%= mfcoffice.ogrn %></td>
      <td><%= link_to 'Посмотреть', mfcoffice_path(mfcoffice.id) %></td>
      <td><%= link_to 'Редактировать', edit_mfcoffice_path(mfcoffice.id) %></td>
      <td><%= link_to 'Удалить', mfcoffice_path(mfcoffice.id),
              method: :delete,
              data: { confirm: 'Вы уверены?' } %></td>
    </tr>
  <% end %>
</table>
